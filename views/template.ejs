<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>Ajax</title>
	<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
	<script>
		$(function(){
			$('#btn').click(function(){
				$.ajax({
					type: 'POST',
					url: '/ajax',
					dataType: 'json',
					success: function(data){
							$('#msg').html(data.msg);
					},
					error: function(){
					}
				});
			});
		});
	</script>
</head>
<body>
	<form>
		<div id="msg"><div>
		<button id="btn" type="button">Send</button>
	</form>
</body>
</html>
